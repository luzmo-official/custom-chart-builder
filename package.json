{
  "name": "luzmo-custom-chart-builder",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:angular": "ng serve -o --proxy-config proxy.conf.json",
    "start:bundle-server": "node server.js",
    "start:watcher": "node scripts/watch.js",
    "start": "concurrently \"npm run start:angular\" \"npm run start:bundle-server\" \"npm run start:watcher\" --kill-others --names \"ANGULAR,BUNDLE,WATCHER\" --prefix-colors \"blue,green,yellow\" --prefix \"[{name}]\"",
    "build": "cd projects/custom-chart && npm run build && cd ../.. && node scripts/copy-assets.js && npm run archive",
    "archive": "node scripts/archive-bundle.js",
    "validate": "node scripts/validate-manifest.js",
    "postinstall": "cd projects/custom-chart && npm install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.0",
    "@angular/cdk": "^20.0.0",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-browser-dynamic": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@luzmo/analytics-components-kit": "1.0.1-alpha.38",
    "@luzmo/dashboard-contents-types": "^1.0.18",
    "@luzmo/lucero": "1.0.1-alpha.1",
    "@luzmo/icons": "1.0.1-alpha.1",
    "@ng-bootstrap/ng-bootstrap": "^19.0.0",
    "@ngneat/until-destroy": "^10.0.0",
    "bootstrap": "^5.3.3",
    "lit": "^3.2.1",
    "ngx-cookie-service": "^20.1.0",
    "ngx-json-viewer": "^3.2.1",
    "rxjs": "~7.8.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.0",
    "@angular/cli": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "archiver": "^7.0.1",
    "chokidar": "^4.0.3",
    "concurrently": "^9.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "zod": "^3.24.2"
  }
}
